cpus=$(awk "\$1==\"processor\" {print \$3}" /proc/cpuinfo | wc -l)
cat > ./config.h << EOF
#ifndef __CONFIG_H__
#define __CONFIG_H__
/* this is automatically generated by .configure. */
#define NR_RT_CPUS $cpus
#endif
EOF
