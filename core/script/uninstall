module="resch"
device="resch"
version="$(uname -r)"
kerninc="/usr/src/kernels/$version/include/resch"
userinc="/usr/include/resch"

# invoke rmmod with all arguments we got
/sbin/rmmod $module $*

# Remove stale nodes

rm -f /dev/${device}
rm -rf $kerninc
rm -rf $userinc
